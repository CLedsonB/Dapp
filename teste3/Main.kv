#Costantes Tela inicial

<InicioButton@Button>:
    background_color: ('#22FF22')
    font_size: '18sp'
    size_hint: 1, 0.25
 
<InicioLabel@Label>:
    color: ('#0F0F0F')
    font_size: '20sp'
    size_hint: 1, 0.20
    markup: True


# Constantes Tela corrente de partida

<CorrenteInput@TextInput>:
    multiline: False
    write_tab: False
    input_filter: 'float'
    font_size: 30
    size_hint: None, None
    width: 150
    heigth: 100
    
<CorrenteLabel@Label>:
    color: ('#0F0F0F')
    font_size: '18sp'

<UnidadesLabel@Label>:
    color: ('#0F0F0F')
    font_size: '16sp'
    
<CorButton@Button>:
    background_color: ('#22FF22')
    font_size: '18sp'
    
#Constante Sistema de fases

<SistemaDeFases@BoxLayout>:
    orientation: 'horizontal'
    spacing: 5
    padding: 5
    UnidadesLabel:
        text: 'Quantidade\nde Fases:'
        size_hint: 0.5, 0.3
    CheckBox:
        group: 'Sistema de fases'
        color: ('#44FF44')
        active: False
        size_hint: None, 0.3
    UnidadesLabel:
        text: 'F / FF'
        size_hint: None, 0.3
    CheckBox:
        group: 'Sistema de fases'
        color: ('#44FF44')
        active: True
        size_hint: None, 0.3
    UnidadesLabel:
        text: 'FFF'
        size_hint: None, 0.3
          
# Fundo Branco e espacamento do App

<EditBoxLayout@BoxLayout>:
    orientation: 'vertical'
    spacing: 10
    padding: 10
    canvas.before:
        Color:
            rgba: 0.95, 0.95, 0.95, 1
        Rectangle:
            pos: self.pos
            size: self.size

<EditGridLayout@GridLayout>:
    spacing: 0
    padding: 20
    cols: 1
    canvas.before:
        Color:
            rgba: 0.9, 0.9, 0.9,  1
        Rectangle:
            pos: self.pos
            size: self.size

#Layout Telas

<jInicio>:     
    EditBoxLayout:          
        InicioLabel:
            text: '[b]Dimensionamento[/b]'
        
        InicioButton:        
            text: 'Condutor'
            on_press: root.manager.current = 'jCondutor'                        

        InicioButton:
            text: 'Contator'
                        
        InicioButton:
            text: 'Fusível'
            on_press: root.manager.current = 'jFusivel'
                    
        InicioButton:
            text: 'Relé Térmico'
                
        InicioLabel:
            text: '[b]Extra[/b]'
                        
        InicioButton:
            text: 'Corrente Nominal do motor'
            on_press: root.manager.current = 'jCorrenteN'
            
        InicioButton:
            text: 'Corrente de Partida do motor'
            on_press: root.manager.current = 'jCorrenteP'
                    
        InicioLabel:
            text: '   '
            size_hint: 1, 0.01

#Tela condutor            
<jCondutor>:       
    EditBoxLayout:
        BoxLayout:
            orientation: 'horizontal'
            size_hint_y: 0.2
            
            CorButton:
                text: '<--'
                pos: 0, 0
                size_hint_x: None
                width: 100
                on_press: root.manager.current = 'jInicio'
            CorrenteLabel:
                markup: True
                text: '[b]Dimensionamento de Condutores[/b]'
        Label:
            text: ' '
            size_hint_y: 0.1

        InicioButton:        
            text: 'Método da Corrente'
        InicioButton:
            text: 'Método da Queda de Tensão'
            on_press: root.manager.current = 'jcQueda'
        InicioButton:
            text: 'Método Ampere x Metro'
            on_press: root.manager.current = 'jcAxm'
        Label:
            text: ' '

# Metodo da queda de tensão            
<jcQueda>:
    EditGridLayout:
        GridLayout:
            cols: 3
     
            CorButton:
                text: '<--'
                pos: 0, 0
                size_hint: None, None
                width: 100
                height: 120
                on_press: root.manager.current = 'jCondutor'
            CorrenteLabel:
                markup: True
                text: '[b]Método da queda de tensão [/b]'
            Label:
                text: ' '
                size_hint: 1, 0.2
                
            UnidadesLabel:
                text: 'Resistividade\ndo material: '
            CorrenteInput:
            CorrenteLabel:
                text: 'р'
                size_hint: None, 0.6
                
            UnidadesLabel:
                text: 'Corrente de\ntrabalho: '
            CorrenteInput:
            CorrenteLabel:
                text: 'A'
                size_hint: None, 0.6
                
            UnidadesLabel:
                text: 'Fator\nde potencia: '
            CorrenteInput:
            CorrenteLabel:
                text: 'ф'
                size_hint: None, 0.6
                
            UnidadesLabel:
                text: 'Comprimento\ndo Condutor: '
            CorrenteInput:
            CorrenteLabel:
                text: 'm'
                size_hint: None, 0.6

            UnidadesLabel:
                text: 'Queda de\nTensão: '
            CorrenteInput:
            CorrenteLabel:
                text: '%'
                size_hint: None, 0.6
    
        SistemaDeFases:                
        Label:
            text: ' '
            size_hint_y: 0.8
        CorButton:
            size_hint: 1, 0.3
            spacing: 10
            text: 'Calcular'
        Label:
            text: ' '
            size_hint: 1, 0.6
                
# Tela metodo Ampere x Metro
<jcAxm>:
    EditGridLayout:
        GridLayout:
            cols: 3
     
            CorButton:
                text: '<--'
                pos: 0, 0
                size_hint_x: None
                width: 100
                height: 40
                on_press: root.manager.current = 'jCondutor'
            CorrenteLabel:
                markup: True
                text: '[b]Método Ampere x Metro[/b]'
            Label:
                text: ' '
                size_hint: 1, 0.3
                
            UnidadesLabel:
                text: 'Corrente de\nPartida do motor: '
            CorrenteInput:
            CorrenteLabel:
                text: 'A'
                size_hint: 0.3, None
                
            UnidadesLabel:
                text: 'Comprimento do\nCondutor: '
            CorrenteInput:
            CorrenteLabel:
                text: 'm'
                size_hint: 0.3, None          
        Label:
            text: ' '
            size_hint: 1, 0.3
        CorButton:
            size_hint: 1, 0.2
            width: 200
            height: 100
            spacing: 10
            text: 'Calcular'
        Label:
            text: ' '
            size_hint: 1, 0.3
 
